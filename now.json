{
  "name": "image-uploader",
  "version": 1,
  "builds": [{
    "src": "server.js",
    "use": "@now/node"
    }],
    "routes": [{
      "src": "/.*",
      "dest": "server.js"
      }],
      "env": {
        "mongoURI": "process.env.mongoURI",
        "CLOUDINARY_API_KEY": "process.env.CLOUDINARY_API_KEY",
        "CLOUDINARY_API_SECRET": "process.env.CLOUDINARY_API_SECRET"
      },
      "build": {
    "env": {
      "mongoURI": "process.env.mongoURI",
      "CLOUDINARY_API_KEY": "process.env.CLOUDINARY_API_KEY",
      "CLOUDINARY_API_SECRET": "process.env.CLOUDINARY_API_SECRET"
    }
  }
}